<!doctype html>
<html lang="en">
<head>
    <title>{{ title }}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="icon" href="favicon.ico" type="image/x-icon" />
    <link rel="stylesheet" href="vendor/twbs/bootstrap/docs/dist/css/bootstrap.min.css">
    <script src="vendor/components/jquery/jquery.min.js"></script>
    <script src="vendor/twbs/bootstrap/docs/dist/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="css/base.css" >

    <!-- INCLUDE CSS -->
    {% for f in cssfile %}
        <link rel="stylesheet" href="{{ f }}" >
    {% endfor %}

    <!-- INCLUDE SCRIPTS -->
    {% for s in scripts %}
        <script src="{{ s }}"></script>
    {% endfor %}

</head>

<body>

<!-- WRAPPER (hack to make footer stick to bottom) -->
<div id="wrapper">

    <!-- NAVBAR AND PAGE BODY -->
    <div id="content">

        <div class="container-fluid">

            {% include("navbar.twig") %}

            {% block content %}{% endblock content %}

        </div>

    </div>

    <!-- FOOTER -->
    <div id="footer">
        <footer>
            &copy; Příliš žluťoučký kůň úpěl ďábelské ódy.
        </footer>
    </div>
</div>

{% block javascripts %}{% endblock %}

</body>
</html>